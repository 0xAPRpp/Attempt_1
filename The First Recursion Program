//Overloaded Functions
# include <iostream>
# include <string>
# include <vector>

//Prototype
int factorial( int num);

int main()
{	
	int x{};
	std::cin >> x;

	std::cout << factorial(x) << std::endl;

	system("pause");
	return 0;
}

int factorial(int num)
{
	if (num == 2)
		return num;
	else
		return num * factorial(num-1);
	
}
