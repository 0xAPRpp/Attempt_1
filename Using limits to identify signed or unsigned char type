# include <iostream>
# include <limits>

using namespace std;

int main()
{
	
	cout << static_cast<int>(std::numeric_limits<char>::min()) <<endl;
	cout << std::numeric_limits<char>::min() << endl;
	cout << static_cast<int>(std::numeric_limits<signed char>::min()) << endl;
	cout << std::numeric_limits<signed char>::min() << endl;
	cout << static_cast<int>(std::numeric_limits<unsigned char>::min()) << endl;
	cout << std::numeric_limits<unsigned char>::min() << endl;

	return 0;
}
